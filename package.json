{
  "name": "react-wood-duck",
  "version": "0.1.40",
  "description": "The CWDS component library (react-wood-duck) is for all CWDS digital service teams to contribute to and use in order to provide a consistent look, feel, and behavior for the overall CWDS application.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ca-cwds/react-wood-duck.git"
  },
  "author": "Naga Pradeep Gajula",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ca-cwds/react-wood-duck/issues"
  },
  "homepage": "https://github.com/ca-cwds/react-wood-duck",
  "keywords": [
    "react-component",
    "react"
  ],
  "scripts": {
    "prepublish": "babel --plugins transform-es2015-modules-umd src --out-dir ./dist && node-sass src/styles/application.scss dist/styles/application.scss && node-sass src/styles/application.scss dist/styles/application.css && echo 'all dist copied.'",
    "lint": "eslint ./{src,stories}",
    "lintfix": "eslint ./src --fix",
    "lint:configtest": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettier:check": "prettier --list-different --config ./.prettierrc \"{src,stories}/**/*.js?(x)\"",
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=production webpack",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.11",
    "@storybook/addon-info": "^3.2.12",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react": "^3.2.11",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.0",
    "jasmine": "^2.7.0",
    "jasmine-core": "^2.7.0",
    "jsdom": "^8.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "path": "^0.12.7",
    "prettierrc": "^0.0.0-5",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^1.17.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
